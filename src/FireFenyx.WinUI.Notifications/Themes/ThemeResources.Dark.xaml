<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="NotificationSolidBrush" Color="#333333" />
    <SolidColorBrush x:Key="NotificationForegroundBrush" Color="#FFFFFF" />

    <AcrylicBrush
        x:Key="NotificationAcrylicBrush"
        FallbackColor="#202020"
        TintColor="#202020"
        TintOpacity="0.6" />

    <SolidColorBrush x:Key="NotificationMicaBrush" Color="#202020" />
</ResourceDictionary>

